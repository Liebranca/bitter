entry main;

reg vars;
  char2 x "hello, world!";
  char  y 0x00;
  char  z y-x;

proc main;
  wed   char;
  cpy   y,y-x;
  cpy   y+1,[y];

proc loop;
  wed   char;
  cpy   y+2,[x+[y]-1];

  sub   y+1,![y+2];
  dec   y;

  jif   end,?[y+2];
  jif   loop,[y]<0xFF;

proc end;
  wed   long;
  exit  y+2;
