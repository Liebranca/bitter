$$$$$$$$$$$$$$$$

entry main;

reg vars;
  char  x "hello, world!";
  char  y 0x00;
  char  c 0x00;

proc main;
  mov   c,x:+[y];
  eif   [c],end;

  cpy   y+1,'a'<=[c];
  and   y+1,'z'>=[c];

  mul   y+1,0x20;
  sub   c,[y+1];

  wap   x+:[y],c;

  inc   y;
  jif   main,13>[y];

proc end;
  wed   long;
  exit  x;
